<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="690.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="xogameserver.FXMLDocumentController">
   <children>
      <ImageView fitHeight="690.0" fitWidth="250.0" pickOnBounds="true">
         <image>
            <Image url="@../assets/2.jpg" />
         </image>
      </ImageView>
      <PieChart fx:id="playersPieChart" layoutX="307.0" layoutY="77.0" legendSide="LEFT" prefHeight="475.0" prefWidth="549.0" />
      <ImageView fx:id="refreshButton" fitHeight="50.0" fitWidth="50.0" layoutX="875.0" layoutY="25.0" onMouseClicked="#refreshPieChart" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/refresh.png" />
         </image>
      </ImageView>
      <ImageView fx:id="startButton" fitHeight="70.0" fitWidth="225.0" layoutX="335.0" layoutY="586.0" onMouseClicked="#startServer" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/start.png" />
         </image>
      </ImageView>
      <ImageView fx:id="stopButton" fitHeight="70.0" fitWidth="225.0" layoutX="614.0" layoutY="586.0" onMouseClicked="#stopServer" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/stop.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
